<?xml version="1.0" encoding="UTF-8"?>
<jax-ws xmlns="http://www.netbeans.org/ns/jax-ws/1">
  <services>
    <service name="LoginWS">
      <implementation-class>com.rudysorto.ws.LoginWS</implementation-class>
    </service>
    <service name="AppsMovilesWS">
      <implementation-class>com.rudysorto.ws.AppsMovilesWS</implementation-class>
    </service>
    <service name="GuardarRegistrosMMWS">
      <implementation-class>com.rudysorto.ws.GuardarRegistrosMMWS</implementation-class>
    </service>
  </services>
  <clients>
    <client name="AppsMovilesWS">
      <wsdl-url>http://localhost:8080/PaillMoviles-war/AppsMovilesWS?wsdl</wsdl-url>
      <local-wsdl-file>localhost_8080/PaillMoviles-war/AppsMovilesWS.wsdl</local-wsdl-file>
      <package-name forceReplace="true">com.rudysorto.ws_client</package-name>
      <catalog-file>catalog.xml</catalog-file>
      <wsimport-options>
        <wsimport-option>
          <wsimport-option-name>extension</wsimport-option-name>
          <wsimport-option-value>true</wsimport-option-value>
